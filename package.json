{
  "name": "dopeoplesay-alfred-workflow",
  "description": "Do People Say Alfred Workflow",
  "version": "0.0.0",
  "author": "Yasuaki Uechi <y@uechi.io> (https://uechi.io/)",
  "scripts": {
    "build": "pkg . --target node10 --output ./workflow/dps",
    "pack": "yarn build && make package"
  },
  "dependencies": {
    "dopeoplesay-cli": "^2.1.0"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "pkg": "^4.3.7",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0"
  },
  "bin": "index.js",
  "bugs": {
    "url": "https://github.com/uetchy/dopeoplesay-alfred-workflow/issues"
  },
  "homepage": "https://github.com/uetchy/dopeoplesay-alfred-workflow",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "keywords": [
    "dopeoplesay-alfred-workflow"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uetchy/dopeoplesay-alfred-workflow.git"
  }
}
